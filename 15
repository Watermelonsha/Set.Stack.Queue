import java.util.Deque;
import java.util.LinkedList;

public class Main {
    public static void main(String[] args) {
        Deque<Integer> deque = new LinkedList<>();

        deque.offerLast(10);
        deque.offerLast(null); //тут нул
        deque.offerLast(30);

        System.out.println("Deque: " + deque);

        System.out.println(" peek:");
        System.out.println("peek: " + deque.peek());
        System.out.println("peekFirst: " + deque.peekFirst());
        System.out.println("peekLast: " + deque.peekLast());

        System.out.println("Удаляем 10:");
        deque.pollFirst(); // удаляем 10
        System.out.println("peek: " + deque.peek());         //первый нулл
        System.out.println("peekFirsет: " + deque.peekFirst());
        System.out.println("peekLast: " + deque.peekLast());  //30оследн
    }
}
